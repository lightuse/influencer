{
  "name": "influencer-api",
  "version": "1.0.0",
  "description": "TypeScript + TypeSpec driven influencer posts analysis API",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:tsc": "tsc",
    "build:watch": "tsc --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "docs": "typedoc",
    "docs:serve": "pnpm run docs && http-server docs",
    "test:ui": "vitest --ui",
    "cli:import": "node dist/cli/import-csv.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "typespec:compile": "tsp compile schemas/main.tsp",
    "typespec:watch": "tsp compile schemas/main.tsp --watch",
    "api:types": "pnpx openapi-typescript tsp-output/@typespec/openapi3/openapi.yaml -o generated/api-types.ts",
    "api:build": "pnpm run typespec:compile && pnpm run api:types",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "format": "prettier --write . --ignore-path .prettierignore",
    "format:check": "prettier --check . --ignore-path .prettierignore",
    "clean": "rimraf dist tsp-output generated/*.ts",
    "prebuild": "pnpm run api:build && pnpm run db:generate",
    "lint": "eslint . --ext .ts,.js --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "api",
    "influencer",
    "typescript",
    "typespec",
    "postgresql",
    "prisma",
    "pnpm",
    "onion-architecture"
  ],
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "kuromoji": "^0.1.2",
    "multer": "^2.0.2",
    "pg": "^8.11.3",
    "prisma": "^6.15.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.23.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.0",
    "@types/pg": "^8.15.5",
    "@types/request": "^2.48.13",
    "@types/supertest": "^6.0.3",
    "@typespec/compiler": "^1.3.0",
    "@typespec/http": "^1.3.0",
    "@typespec/json-schema": "^1.3.0",
    "@typespec/openapi3": "^1.3.0",
    "@typespec/rest": "^0.73.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "http-server": "^14.1.1",
    "nodemon": "^3.0.1",
    "openapi-typescript": "^7.9.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.12",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "vite": "^7.1.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0"
  },
  "packageManager": "pnpm@9.7.0"
}
